namespace com.linkedin.role

import com.linkedin.common.AuditStamp

/**
  * Information about a ExternalRoleProperties
  */
@Aspect = {
  "name": "overProvisionedRole"
}
record OverProvisionedRole {
  /**
    * Whether role has granted more permission as compared to its usage history 
    * found in audit logs
    */
  @Searchable = {
  "fieldType": "BOOLEAN"
  }
  status: boolean = false
}
 