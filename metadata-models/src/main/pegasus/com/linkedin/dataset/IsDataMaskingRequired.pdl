  */
@Aspect = {
  "name": "isDataMaskingRequired"
}
record IsDataMaskingRequired {
  /**
    * The status will be true if the dataset has sensitive data and 
    * masking policy is not present on that Datasetâ€™s column. 
    */
  @Searchable = {
  "fieldType": "BOOLEAN"
  }
  status: boolean = false
}
 